mwashington/Documents/Development/clients/spwashi.com/packages/d3/react/src/data/components/edges/selectors/svg.ts",[],"/Users/samwashington/Documents/Development/clients/spwashi.com/packages/d3/react/src/data/components/edges/selectors/datum.ts",[],"/Users/samwashington/Documents/Development/clients/spwashi.com/packages/d3/react/src/data/components/nodes/style/index.ts",[],"/Users/samwashington/Documents/Development/clients/spwashi.com/packages/d3/react/src/data/components/edges/style/index.ts",[],"/Users/samwashington/Documents/Development/clients/spwashi.com/packages/d3/react/src/app/components/config/components/input/InputWidgetFactory.tsx",["240"],"import {EnumVizConfigItem, RangeVizConfigItem, VizConfigItem} from '../../config/types';\nimport {RangeInputControl, RangeInputController, RangeInputValue} from './RangeInputWidget';\nimport React, {useEffect} from 'react';\nimport {InputUpdateHandler} from './types';\nimport {InputWidgetWrapper} from './Wrapper';\n\ninterface InputItemParams<T = any> {\n    item: VizConfigItem;\n    value: number;\n    index: string;\n    onChange: InputUpdateHandler<T>;\n}\n\nfunction ToggleInput({value, onChange}: { value: boolean, onChange: (state: boolean) => void }) {\n    return <input type=\"checkbox\" checked={value} onChange={event => onChange(event.target.checked)}/>\n}\n\nfunction EnumInput({value, onChange, options}: { value: any, onChange: (state: any) => void, options: any[] }) {\n    return (\n        <select value={value} onChange={e => onChange(e.target.value)}>\n            {options.map(option => <option key={option} value={option}>{option}</option>)}\n        </select>\n    )\n}\n\nexport default function InputWidgetFactory({item, value, index, onChange}: InputItemParams) {\n    const {type, state} = item;\n    let valueComponent: JSX.Element | any;\n    let inputComponent: JSX.Element | any;\n\n    useEffect(() => {\n        onChange(state)\n    }, [state]);\n\n\n    switch (type) {\n        case 'range': {\n            const {step, min, max}               = item as RangeVizConfigItem;\n            let controller: RangeInputController = [value || 0, onChange];\n            valueComponent                       = <RangeInputValue value={controller[0]} setValue={controller[1]}/>;\n            inputComponent                       = <RangeInputControl step={step}\n                                                                      min={min}\n                                                                      max={max}\n                                                                      controller={controller}/>;\n            break;\n        }\n        case 'toggle': {\n            valueComponent = value ? 'yes' : 'no';\n            inputComponent = <ToggleInput value={!!value} onChange={onChange}/>\n            break;\n        }\n        case 'enum': {\n            valueComponent  = value;\n            const {options} = item as EnumVizConfigItem;\n            inputComponent  = <EnumInput value={value} options={options} onChange={onChange}/>\n            break;\n        }\n        default:\n            return <>{`undefined input type ${type}`}</>;\n    }\n\n    return <InputWidgetWrapper title={index} input={inputComponent} value={valueComponent}/>\n}","/Users/samwashington/Documents/Development/clients/spwashi.com/packages/d3/react/src/data/components/clusters/behaviors/drag.ts",[],"/Users/samwashington/Documents/Development/clients/spwashi.com/packages/d3/react/src/data/components/clusters/behaviors/zoom.ts",[],"/Users/samwashington/Documents/Development/clients/spwashi.com/packages/d3/react/src/data/components/clusters/selectors/svg.ts",[],"/Users/samwashington/Documents/Development/clients/spwashi.com/packages/d3/react/src/data/components/clusters/style/index.ts",[],"/Users/samwashington/Documents/Development/clients/spwashi.com/packages/d3/react/src/data/components/clusters/selectors/datum.ts",[],"/Users/samwashington/Documents/Development/clients/spwashi.com/packages/